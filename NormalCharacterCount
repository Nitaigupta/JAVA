package Recursion;

import java.util.*;

/**
 * Practise
 */
public class Practise {

  public static void main(String[] args) {
  String s = "abc";
  int arr[] = new int[26];
  for (char c : s.toCharArray()) {
    arr[c-'a']++;
    
  }
  System.out.println(Arrays.toString(arr));
    
   }
   
  }
