import java.util.*;
public class RandomStringGeneration {
    public static void main(String[] args) {
        System.out.println(Generation(8));
        
    }
    static StringBuffer Generation(int size){
        StringBuffer s = new StringBuffer();
        Random r = new Random();
        for (int i = 0; i < size; i++) {
            int newChar = 97+(int)(r.nextFloat()*26);
            s.append((char)newChar);

            
        }
        return s;

    }

    
}
